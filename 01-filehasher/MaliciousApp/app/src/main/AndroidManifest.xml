<?xml version='1.0'?>
<manifest xmlns:android='http://schemas.android.com/apk/res/android' android:versionCode='0' android:versionName='0'>
    <uses-sdk android:targetSdkVersion='33' />

    <application android:label='MaliciousApp' android:theme='@style/Theme.AppCompat.DayNight'>
        <activity android:name='.MainActivity' android:exported='true'>
            <intent-filter>
                <category android:name='android.intent.category.LAUNCHER' />
                <action android:name='android.intent.action.MAIN' />
            </intent-filter>
        </activity>

        <activity android:name='.HashFile' android:exported='true'>
            <intent-filter>
                <action android:name='com.mobiotsec.intent.action.HASHFILE' />
                <category android:name='android.intent.category.DEFAULT' />
                <data android:mimeType='text/plain' />
            </intent-filter>
        </activity>
    </application>
</manifest>
